<div region="center" split="true" border="false">    
    <table id="masters" class="easyui-datagrid" 
        url="/StockMoveSearch/masters/" border="false" singleSelect="true"
        fit="true" toolbar="#tool"  pagination="true" rownumbers="true">
        <thead>
            <tr>
                <th field="MoveBillNo" width="120" align="center">移库单号</th>
                <th field="WarehouseName" width="120" align="center">仓库名称</th>
                <th field="MakeDate" width="120" align="center">制单日期</th>
                <th field="OperatePerson" width="120" align="center">操作员</th>
                <th field="OperateStatus" width="120" align="center">处理状态</th>
                <th field="CheckPerson" width="120" align="center">审核人</th>
                <th field="CheckDate" width="120" align="center">审核日期</th>
                <th field="Remark" width="120" align="center">备注</th>
            </tr>
        </thead>
    </table>
</div>
<div region="south" split="true" border="false" style="height: 250px; border-top-style: solid; border-top-width: 1px; border-top-color: 99BBE8;">
<table id="details" class="easyui-datagrid" 
        url="/StockMoveSearch/Details/" border="false" singleSelect="true"
        fit="true"  pagination="true" rownumbers="true">
        <thead>
            <tr>
                <th field="IdNo" width="80" align="center">序号</th>
                <th field="ProduceCode" width="100" align="center">产品代码</th>
                <th field="ProduceName" width="140" align="center">产品名称</th>
                <th field="PlaceCode" width="100" align="center">货位编码</th>
                <th field="PlaceName" width="140" align="center">货位名称</th>
                <th field="UnitCode" width="100" align="center">单位编码</th>
                <th field="UnitName" width="140" align="center">单位名称</th>
                <th field="Quantity" width="80" align="center">分配数量</th>
                <th field="WorkStatus" width="80" align="center">作业状态</th>
            </tr>
        </thead>
    </table>
</div>    