<script type="text/javascript">
    function auditClick() {
        var row = $('#details').datagrid('getSelected');
        if (row) {
            isInBillMasterSaved = false;
            isInBillMasterCanEdit = true;
            if (row.Status != "已录入") {
                $.messager.confirm('错误', '该记录已审核无需重复审核！');
            } else {
                $('#dlg').dialog('open').dialog('setTitle', '审核入库单据信息');
                $('#fm').form('load', row);
                url = '/StockInBill/Audit/?BillNo='+row.BillNo;
            }
        } else {
            $.messager.confirm('错误', '没有选择行！');
        }
    };
</script>