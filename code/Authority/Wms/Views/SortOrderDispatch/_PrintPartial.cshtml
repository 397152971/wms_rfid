<script type="text/javascript">
    function printClick() {
        var orderDate = null
          , sortingLineCode = null;

        if (document.getElementById('searchOrderDate') == null) {
            orderDate = "";
        }
        else {
            orderDate = $('#searchOrderDate').datebox('getValue');
        }

        if (document.getElementById('SortingLineCode') != null) {
            sortingLineCode = "";
        }
        else {
            sortingLineCode = $('#SortingLineCode').combogrid('getValue');
        }
        
        window.location.href = "/SortOrderDispatch/CreateExcelToClient/"
                                + "?orderDate=" + orderDate
                                + "&sortingLineCode=" + sortingLineCode;
    }
</script>
