<script type="text/javascript">
    function deleteClick() {
        var productRow = $('#details').datagrid('getSelected');
        if (productRow) {
            $('#details').datagrid({ url: '/DefaultProductSet/CellDel/?productCode=' + productRow.ProductCode });
        }
        $('#details').datagrid({ url: '/DefaultProductSet/GetProductCell/' });
        $('#details').datagrid('reload');
    }
</script>
