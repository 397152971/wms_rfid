<script type="text/javascript">
    function printClick() {
        var row = $('#Tree').tree('getSelected');
        if (row) {
            var rowType=row.attributes.AttributeId;
            var rowId = row.id
            var rowUnitType = row.attributes.AttributeUnit
            var rowProductCode = row.attributes.AttributeProduct;
            
            window.location.href = "/Distribution/CreateExcelToClient/"
                                    + "?type=" + rowType
                                    + "&id=" + rowId
                                    + "&unitType=" + rowUnitType
                                    + "&productCode=" + rowProductCode;
        }
        else {
            $.messager.alert("系统提示","未选中树形菜单","info");
        }
    }
</script>