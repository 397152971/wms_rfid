<script type="text/javascript">
    function printClick() {
        var row = $('#Tree').tree('getSelected');
        if(row)
        {
                    var AttributeId = row.attributes.AttributeId;
                    var AttributeProduct = row.attributes.AttributeProduct;
                    var AttributeUnit = row.attributes.AttributeUnit;
                    window.location.href = "/Distribution/CreateExcelToClient/"
                                    + "?type=" + row.attributes.AttributeId
                                    + "&id=" + row.id
                                    + "&unitType=" + row.attributes.AttributeUnit
                                    + "&productCode=" + row.attributes.AttributeProduct;
        }
    }
</script>