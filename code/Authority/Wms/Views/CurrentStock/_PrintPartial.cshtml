//        $('#details').datagrid({
//            pageNumber: 1,
//            pageSize: 10
//        });
//        $("#details").datagrid('reload');
//        $('#dlg-search').dialog('close');

        window.location.href = "/CurrentStock/CreateExcelToClient/?productCode=" + $('#productCode').val() + "&ware=" + ware + "&area=" + area + "&unitType=" + $('#unit_Type').combo('getValue');
    }
</script>
