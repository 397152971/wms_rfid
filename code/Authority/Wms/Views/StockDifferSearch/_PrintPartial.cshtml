<script type="text/javascript">
    function printClick() {

        var row = $('#details').datagrid('getSelected');
        if (row) {
            var rowBill = null;
            if (row.BillNo == null) { rowBill = ""; } else { rowBill = row.BillNo; }
            window.location.href = '/ProfitLossBillDetail/CreateExcelToClient/?billNo=' + rowBill;
        } else {
            $.messager.alert('系统提示', '未选中行', "info");
        }
    }
</script>
