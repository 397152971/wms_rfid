<script type="text/javascript">
    function changedChecckSearch() {
        begin = $('#beginDate').datebox('getValue');
        end = $('#endDate').datebox('getValue');
        $('#chanedCheckTable').datagrid('options').url = '/CheckBill/CheckChangedDetails/?beginDate=' + begin + '&endDate=' + end;
        $('#chanedCheckTable').datagrid('load');
    }
    
</script>
